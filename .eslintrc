{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "settings": {
      "import/resolver": {
          "node": {
              "extensions": [
                  ".ts",
                  ".js",
                  ".json"
              ]
          }
      },
      "import/extensions": [
          ".js",
          ".ts"
      ]
  },
  "rules": {
      "max-len": "off",
      "no-useless-escape": "off",
      "import/prefer-default-export": "off",
      "consistent-return": "off",
      "no-underscore-dangle": "off",
      "import/extensions": [
          "error",
          "ignorePackages",
          {
              "js": "never",
              "ts": "never"
          }
      ]
  },
}